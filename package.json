{
  "name": "sticky-kit",
  "version": "1.1.3",
  "description": "A jQuery plugin for creating smart sticky elements",
  "main": "dist/sticky-kit.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coffee": "coffee -p -c jquery.sticky-kit.coffee > jquery.sticky-kit.js",
    "closure": "google-closure-compiler --language_in=ECMASCRIPT5 jquery.sticky-kit.js > jquery.sticky-kit.min.js",
    "build": "npm run coffee && npm run closure",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leafo/sticky-kit.git"
  },
  "author": "leafo <leafot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leafo/sticky-kit/issues"
  },
  "homepage": "https://github.com/leafo/sticky-kit#readme",
  "keywords": [
    "jquery",
    "jquery-plugin",
    "sticky"
  ],
  "devDependencies": {
    "coffeescript": "^1.12.7",
    "google-closure-compiler": "^20190325.0.0",
    "jasmine": "^2.5.2",
    "jquery": "^3.1.1"
  }
}
